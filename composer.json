{
    "name": "pkc/video-call",
    "description": "Laravel video calling package using WebRTC and Laravel Reverb for real-time communication",
    "type": "library",
    "keywords": [
        "laravel",
        "webrtc",
        "video-call",
        "laravel-reverb",
        "real-time",
        "video-chat"
    ],
    "homepage": "https://github.com/yourusername/laravel-video-call",
    "license": "MIT",
    "authors": [
        {
            "name": "Roktim Ariyan",
            "email": "laradev.sumon@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/view": "^10.0",
        "laravel/reverb": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "pkc\\VideoCall\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "pkc\\VideoCall\\VideoCallServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 